-- Créer par Furtydorux
local crimgui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Frame2 = Instance.new("Frame")
local crimtext = Instance.new("TextBox")
local makecrim = Instance.new("TextButton")
