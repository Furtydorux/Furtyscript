ouvrir.Parent = ScreenGui
ouvrir.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ouvrir.Position = UDim2.new(0, 0, 0.716569841, 0)
ouvrir.Size = UDim2.new(0, 100, 0, 32)
ouvrir.Active = true
ouvrir.Draggable = true

ouvrir2.Name = "ouvrir 2"
ouvrir2.Parent = ouvrir
ouvrir2.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
ouvrir2.Size = UDim2.new(0, 64, 0, 31)
ouvrir2.Font = Enum.Font.SourceSans
ouvrir2.Text = "ouvrir"
ouvrir2.TextColor3 = Color3.fromRGB(0, 0, 0)
ouvrir2.TextSize = 14.000
ouvrir2.MouseButton1Down:connect(function()
ouvrir.Visible = false
Frame.Visible = true
end)

close.Name = "close"
close.Parent = ouvrir
close.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
close.Position = UDim2.new(0.639999986, 0, 0, 0)
close.Size = UDim2.new(0, 35, 0, 31)
close.AutoButtonColor = false
close.Font = Enum.Font.SourceSans
close.Text = ""
close.TextColor3 = Color3.fromRGB(0, 0, 0)
close.TextSize = 14.000
close.MouseButton1Down:connect(function()
ouvrir.Visible = false
end)

-- Pour créer un script il faut siplement avoir roblox studio et créer un 
-- un "scringui" dans "StarterGui" puis créer deux "Frame" dans la premier frame et il 
-- faudra mettre des boutton, dans la deuxième mettait un nom comme "open" puis mettait
-- un "text boutoon qui s'appelle lui "open 2" ou un truc comme ca
-- et enfin créer des "text Button" dans la première frame qui s'appelle "Frame"
-- a personnnaliser vous mèmes. Allé sur Google et taper "Gui to Lua Converte plugin roblox"
-- Aller sur le lien de roblox et obtener le. Sur roblox studio vous séléctionner ensuite 
-- "ScreenGui" puis vous séléctionnez le plugin prècedemment télécharger
-- Vous convertissez en script. Il faudra maintenant changer quelques petit mot
-- Remplacer "game.Workspace" par "game.CoreGui"
-- 
