-- Script
 
Tutup.MouseButton1Click:connect(function()
SharkBiterr:Destroy()
end)
 
Tunda.MouseButton1Click:connect(function()
if TundaF == 1 then
TundaF = 0
Senjata.Visible = true
Perahu.Visible = true
Hiu.Visible = true
Frame.Size = UDim2.new(0, 395, 0, 84)
else
TundaF = 1
Senjata.Visible = false
Perahu.Visible = false
Hiu.Visible = false
Frame.Size = UDim2.new(0, 395, 0, 20)
end
end)
 
Senjata.MouseButton1Click:connect(function()
if SenjataF == 1 then
SenjataF = 0
Fs.Visible = false
else
SenjataF = 1
Fs.Visible = true
end
end)
 
Perahu.MouseButton1Click:connect(function()
if PerahuF == 1 then
PerahuF = 0
Fp.Visible = false
else
PerahuF = 1
Fp.Visible = true
end
end)
 
Hiu.MouseButton1Click:connect(function()
if HiuF == 1 then
HiuF = 0
Fh.Visible = false
else
HiuF = 1
Fh.Visible = true
end
end)
 
