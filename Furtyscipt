rep1.Size = UDim2.new(0, 197, 0, 26)
rep1.Font = Enum.Font.SourceSans
rep1.Text = "réapparaitre au mème endroit"
rep1.TextColor3 = Color3.fromRGB(0, 0, 0)
rep1.TextSize = 14.000
rep1.MouseButton1Down:connect(function()
	saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "Position souvegarder";
		Text = "Succer";
	})


	local A_1 = "LocalPlayer"
	local Event = game:GetService("Workspace").Remote.loadchar
	Event:InvokeServer(A_1)

	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "Positions sauvegarder";
		Text = "Succer";
	})
end)
rep1.MouseButton1Down:connect(function()
for i,v in pairs(Workspace.Prison_ITEMS.giver:GetChildren()) do
 
lol = Workspace.Remote.ItemHandler:InvokeServer(v.ITEMPICKUP)
print(lol)
end
end)

